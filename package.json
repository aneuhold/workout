{
  "name": "workout",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:sb": "storybook build",
    "preview": "pnpm build && vite preview --port 5173",
    "lint": "eslint --cache",
    "test": "vitest run",
    "check": "svelte-check && pnpm check:circular",
    "check:circular": "madge --circular --extensions ts,svelte src/",
    "check:visual": "madge --image deps.svg --extensions ts,svelte src/",
    "upgrade:core": "pnpm up '@aneuhold/*' --latest",
    "storybook": "storybook dev -p 6006",
    "sub": "pnpm sub:core-ts-api-lib",
    "unsub": "local-npm unsubscribe",
    "sub:core-ts-api-lib": "local-npm subscribe @aneuhold/core-ts-api-lib",
    "sub:core-ts-db-lib": "local-npm subscribe @aneuhold/core-ts-db-lib",
    "sub:eslint-config": "local-npm subscribe @aneuhold/eslint-config",
    "unsub:core-ts-api-lib": "local-npm unsubscribe @aneuhold/core-ts-api-lib",
    "unsub:eslint-config": "local-npm unsubscribe @aneuhold/eslint-config",
    "generate:icons": "ICON=docs/officialAssets/logo-icon-circle.svg && for size in 48 72 96 128 144 168 192; do magick -background none \"$ICON\" -resize ${size}x${size} static/icons/${size}.png; done && cp static/icons/128.png static/favicon.png && echo 'Icons generated from logo-icon SVG'"
  },
  "packageManager": "pnpm@10.28.0",
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "^10.38.0",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@aneuhold/core-ts-api-lib": "^3.0.15",
    "@aneuhold/core-ts-db-lib": "^4.1.4",
    "@aneuhold/core-ts-lib": "^2.3.17",
    "@aneuhold/eslint-config": "^2.0.2",
    "@aneuhold/local-npm-registry": "^0.2.24",
    "@fontsource-variable/inter": "^5.2.8",
    "@internationalized/date": "^3.11.0",
    "@neoconfetti/svelte": "^2.2.2",
    "@storybook/addon-docs": "^10.2.7",
    "@storybook/addon-links": "^10.2.7",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-themes": "^10.2.8",
    "@storybook/sveltekit": "^10.2.7",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tabler/icons-svelte": "^3.36.1",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3-scale": "^4.0.9",
    "@types/node": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "bits-ui": "^2.15.5",
    "clsx": "^2.1.1",
    "d3-scale": "^4.0.2",
    "eslint": "^9.0.0",
    "eslint-plugin-storybook": "10.2.7",
    "jsdom": "^28.0.0",
    "layerchart": "2.0.0-next.43",
    "madge": "^8.0.0",
    "mode-watcher": "^1.1.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "shadcn": "^3.8.4",
    "smui-theme": "^8.0.3",
    "storybook": "^10.2.7",
    "svelte": "^5.50.0",
    "svelte-check": "^4.3.6",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-node-polyfills": "^0.25.0",
    "vitest": "^4.0.18"
  }
}
