{
  "name": "dashboard",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm theme && vite dev",
    "build": "pnpm theme && vite build",
    "build:sb": "pnpm theme && storybook build",
    "preview": "pnpm build && vite preview --port 5173",
    "lint": "eslint",
    "theme": "smui-theme compile static/smui.css -i src/globalStyles -i node_modules/.pnpm/node_modules -i node_modules",
    "test": "vitest run",
    "check": "svelte-check && pnpm check:circular",
    "check:circular": "madge --circular --extensions ts,svelte src/",
    "check:visual": "madge --image deps.svg --extensions ts,svelte src/",
    "upgrade:core": "pnpm up '@aneuhold/*' --latest",
    "upgrade:smui": "pnpm up '@smui/*' '@smui-extra/*' smui-theme",
    "storybook": "pnpm theme && storybook dev -p 6006",
    "sub": "pnpm sub:core-ts-api-lib",
    "unsub": "local-npm unsubscribe",
    "sub:core-ts-api-lib": "local-npm subscribe @aneuhold/core-ts-api-lib",
    "sub:core-ts-db-lib": "local-npm subscribe @aneuhold/core-ts-db-lib",
    "sub:eslint-config": "local-npm subscribe @aneuhold/eslint-config",
    "unsub:core-ts-api-lib": "local-npm unsubscribe @aneuhold/core-ts-api-lib",
    "unsub:eslint-config": "local-npm unsubscribe @aneuhold/eslint-config"
  },
  "scriptsComments": {
    "theme": "This has the extra parameters for node_modules so it works with pnpm. Otherwise it fails when SASS tries to resolve imports."
  },
  "packageManager": "pnpm@10.28.0",
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "^10.32.1",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@aneuhold/core-ts-api-lib": "^3.0.9",
    "@aneuhold/core-ts-db-lib": "^4.0.4",
    "@aneuhold/core-ts-lib": "^2.3.16",
    "@aneuhold/eslint-config": "^1.3.21",
    "@aneuhold/local-npm-registry": "^0.2.23",
    "@fontsource/fira-mono": "^5.2.7",
    "@neoconfetti/svelte": "^2.2.2",
    "@smui-extra/accordion": "^8.0.3",
    "@smui-extra/autocomplete": "^8.0.3",
    "@smui/button": "^8.0.3",
    "@smui/card": "^8.0.3",
    "@smui/checkbox": "^8.0.3",
    "@smui/chips": "^8.0.3",
    "@smui/circular-progress": "^8.0.3",
    "@smui/common": "^8.0.3",
    "@smui/dialog": "^8.0.3",
    "@smui/drawer": "^8.0.3",
    "@smui/fab": "^8.0.3",
    "@smui/form-field": "^8.0.3",
    "@smui/icon-button": "^8.0.3",
    "@smui/list": "^8.0.3",
    "@smui/menu": "^8.0.3",
    "@smui/menu-surface": "^8.0.3",
    "@smui/paper": "^8.0.3",
    "@smui/segmented-button": "^8.0.3",
    "@smui/select": "^8.0.3",
    "@smui/snackbar": "^8.0.3",
    "@smui/textfield": "^8.0.3",
    "@smui/tooltip": "^8.0.3",
    "@smui/top-app-bar": "^8.0.3",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/sveltekit": "^10.1.11",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.5",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "10.1.11",
    "jsdom": "^27.4.0",
    "madge": "^8.0.0",
    "prettier": "^3.7.4",
    "prettier-plugin-svelte": "^3.4.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "smui-theme": "^8.0.3",
    "storybook": "^10.1.11",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelty-picker": "^6.1.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^4.0.16"
  }
}
